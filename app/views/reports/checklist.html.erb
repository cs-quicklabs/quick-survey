<p class="fs-3"><%= @attempt.participant.name %></p>
<p class="fs-3"><%= @attempt.participant.email %></p>
<p class="fs-3"><%= @attempt.survey.name %></p>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Questions</th>
      <th scope="col">Answer</th>
      <th scope="col">Handle</th>
    </tr>
  </thead>
  <tbody>
    <% @attempt.survey.questions.each do |question| %>
      <tr>
        <th scope="row">1</th>
        <td><%= question.text %></td>
        <td>NO</td>
        <td>@mdo</td>
      </tr>
    <% end %>
  </tbody>
</table>
